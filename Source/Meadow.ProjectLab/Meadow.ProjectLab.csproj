<Project Sdk="Meadow.Sdk/1.1.0">
  <PropertyGroup>
    <Version>1.11.0</Version>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageIcon>icon.png</PackageIcon>
    <Authors>Wilderness Labs, Inc</Authors>
    <TargetFramework>netstandard2.1</TargetFramework>
    <OutputType>Library</OutputType>
    <AssemblyName>ProjectLab</AssemblyName>
    <Company>Wilderness Labs, Inc</Company>
    <PackageProjectUrl>https://github.com/WildernessLabs/Meadow.ProjectLab</PackageProjectUrl>
    <PackageId>Meadow.ProjectLab</PackageId>
    <RepositoryUrl>https://github.com/WildernessLabs/Meadow.ProjectLab</RepositoryUrl>
    <PackageTags>Meadow.ProjectLab,Meadow,ProjectLab,accelerator</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Library for the Meadow ProjectLab IoT accelerator</Description>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <PropertyGroup>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <LangVersion>10.0</LangVersion>
  </PropertyGroup>
  <ItemGroup>
    <None Include="icon.png" Pack="true" PackagePath="" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Meadow.F7" Version="2.1.0.1" />
    <PackageReference Include="Meadow.Foundation.Displays.TftSpi" Version="2.1.0.1" />
    <PackageReference Include="Meadow.Foundation.ICs.IOExpanders.Mcp23xxx" Version="2.1.0.1" />
    <PackageReference Include="Meadow.Foundation.ICs.IOExpanders.Sc16is7x2" Version="2.1.0.1" />
    <PackageReference Include="Meadow.Foundation.Sensors.Atmospheric.Bme68x" Version="2.1.0.1" />
    <PackageReference Include="Meadow.Foundation.Sensors.Hid.Xpt2046" Version="2.1.0.1" />
    <PackageReference Include="Meadow.Foundation.Sensors.Light.Bh1750" Version="2.1.0.1" />
    <PackageReference Include="Meadow.Foundation.Sensors.Motion.Bmi270" Version="2.1.0.1" />
  </ItemGroup>
</Project>
